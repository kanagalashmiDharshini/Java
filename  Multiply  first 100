public class MultiplyFirst100Numbers {
    public static void main(String[] args) {
        long result = 1; // Initialize result to 1 as multiplication's identity
        
        for (int i = 1; i <= 100; i++) {
            result *= i; // Multiply each number with the accumulated result
        }
        
        System.out.println("Product of the first 100 numbers: " + result);
    }
}
